@model IEnumerable<PCartWeb.Models.UserOrder>

@{
    ViewBag.Title = "OrderHistory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>OrderHistory</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoopId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderCreated_at)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModeOfPay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CommissionFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Delivery_fee)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoopId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderCreated_at)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModeOfPay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommissionFee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Delivery_fee)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
