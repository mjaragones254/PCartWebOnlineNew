@model IEnumerable<PCartWeb.Models.CoopDetailsModel>

@{
    ViewBag.Title = "ViewPendingCoop";
}

<h2>ViewPendingCoop</h2>
<br/>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CoopName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coop_Created)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CoopName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Coop_Updated)
        </td>
        <td>
            @Html.ActionLink("Details", "DetailsCoopReq", new { id=item.Id })
        </td>
    </tr>
}

</table>
